<?xml version="1.0" encoding="UTF-8"?>

<fileset-config file-format-version="1.2.0" simple-config="false" sync-formatter="false">
  <local-check-config name="joblift.de checkstyle checks" location="checkstyle.xml" type="project" description="">
    <additional-data name="protect-config-file" value="true"/>
  </local-check-config>
  <fileset name="all" enabled="true" check-config-name="joblift.de checkstyle checks" local="true">
    <file-match-pattern match-pattern="src/((main)|(test))/java/.*\.java" include-pattern="true"/>
  </fileset>
</fileset-config>
